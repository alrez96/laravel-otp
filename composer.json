{
    "name": "alrez96/laravel-otp",
    "description": "OTP login system for the Laravel framework.",
    "keywords": [
        "alrez96",
        "laravel",
        "otp",
        "authentication",
        "otp-authentication",
        "laravel-otp",
        "one-time-password"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alireza Rostami",
            "email": "mail.alrez96@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Alrez96\\LaravelOtp\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alrez96\\LaravelOtp\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alrez96\\LaravelOtp\\OtpServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}